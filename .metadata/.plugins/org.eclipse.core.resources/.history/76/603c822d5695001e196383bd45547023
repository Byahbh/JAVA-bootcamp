package exec_sec4;

import java.util.Scanner;

public class Exercicio_01 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		
		int soma1= num1 + num2;
		int soma2= num1 * num2;
		int soma3= num1 / num2;
		
		
		System.out.println("A soma 1 Ã©: " + soma1);
		

		
		
		
		sc.close();
	}

	
}
